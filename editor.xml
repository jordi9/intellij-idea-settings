<application>
  <component name="CodeInsightSettings">
    <EXCLUDED_PACKAGE NAME="java.rmi" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern useCustomColors="true" pattern="\b(yolo)\b.*">
      <option name="FOREGROUND" value="fabada" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\b(given|when|then)\b:.*">
      <option name="FOREGROUND" value="fabaef" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
  </component>
</application>